{
    "name": "czim/monolog-json-context",
    "description": "JSON Context for Monolog",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "opensource@czim.nl"
        }
    ],
    "keywords": [ "monolog", "formatting", "json" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": "^8.0",
        "monolog/monolog": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Czim\\MonologJsonContext\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\MonologJsonContext\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
